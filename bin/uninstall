#!/bin/sh

if [ "$(id -u)" != "0" ]; then
    printf "ERROR: This script must be run as root!\n"
    exit 1
fi

rm -fv /bin/svdss && printf "Successfully uninstalled svdss\n"
rm -fv /bin/svddw && printf "Successfully uninstalled svddw\n"
rm -fv /bin/svdpanel && printf "Successfully uninstalled svdpanel\n"

