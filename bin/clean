#!/usr/bin/env sh


if [ "$(uname)" = "Darwin" ]; then
    qmake -spec darwin-g++ Notifications.pro
    make distclean
    qmake -spec darwin-g++ TestSS.pro
    make distclean
    qmake -spec darwin-g++ Panel.pro
    make distclean
    qmake -spec darwin-g++ TheSS.pro
    make distclean
else
    qmake Notifications.pro
    make distclean
    qmake TestSS.pro
    make distclean
    qmake Panel.pro
    make distclean
    qmake TheSS.pro
    make distclean
fi

rm -fv src/*.a
